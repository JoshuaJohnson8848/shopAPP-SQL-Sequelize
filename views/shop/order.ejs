<%- include('../includes/head.ejs') %>
<body>
  <head>
    <%- include('../includes/nav.ejs') %>
  </head>
  <main>
    <% if(orders.length<=0) {%>
    <h1>Nothing Here!</h1>
    <% } else { %>
    <ul>
      <% orders.forEach(order => { %>
      <li>
        <h1># <%= order.id %></h1>
        <ul>
          <% order.products.forEach(product => { %>
          <li>
            <p><%= product.title %> (<%= product.orderItems.quantity %>)</p>
          </li>
          <% }); %>
        </ul>
      </li>
      <% }); %>
    </ul>
    <% } %>
  </main>
</body>
<%- include('../includes/end.ejs') %>
